<h1><%= t ".title" %></h1>

<div id="contacts">
    <% @contacts.each do |contact| %>
        <div>
            <% if contact.url.present? %>
                <%= link_to contact.name, contact.safe_url.to_s %>
            <% else %>
                <%= contact.name %>
            <% end %>        
        </div>
    <% end %>
</div>